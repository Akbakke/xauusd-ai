{
  "version": "v1",
  "created_at": "2026-01-26T13:30:00Z",
  "description": "Multi-head XGB output contract for V10_CTX pipeline",
  "sessions": ["EU", "US", "OVERLAP"],
  "optional_sessions": ["ASIA"],
  "n_classes": 3,
  "class_labels": ["LONG", "SHORT", "FLAT"],
  "outputs_per_head": {
    "p_long_xgb": {
      "description": "Probability of LONG class",
      "range": [0.0, 1.0],
      "dtype": "float32"
    },
    "p_short_xgb": {
      "description": "Probability of SHORT class",
      "range": [0.0, 1.0],
      "dtype": "float32"
    },
    "p_flat_xgb": {
      "description": "Probability of FLAT class",
      "range": [0.0, 1.0],
      "dtype": "float32"
    },
    "uncertainty_xgb": {
      "description": "Entropy of [p_long, p_short, p_flat] normalized by log(3)",
      "range": [0.0, 1.0],
      "dtype": "float32"
    }
  },
  "injection_channels_template": [
    "p_long_xgb_{session}",
    "p_short_xgb_{session}",
    "p_flat_xgb_{session}",
    "uncertainty_xgb_{session}"
  ],
  "injection_mode": "current_session",
  "injection_notes": "Phase 1: inject only current session head. Phase 2: inject all heads.",
  "invariants": {
    "sum_probs_equals_one": true,
    "no_nan_inf": true,
    "no_post_gate": true
  },
  "schema_hash": "multihead_v1_3class"
}
